<head>
  <meta charset="utf-8">
  <meta name="generator" content="{{ eleventy.generator }}">
  <meta name="viewport" content="width=device-width, initial-scale=1.0">
  <title>{{ title or metadata.title }}</title>
  <meta name="description" content="{{ metaDescription or eleventyComputed.metaDescription or metadata.description }}">
  link href="css/bootstrap.min.css" rel="stylesheet" type="text/css" />
  {% set css %}
    {% include "assets/css/materialdesignicons.min.css" %}
    {% include "assets/css/owl.carousel.min.css" %}
    {% include "assets/css/owl.theme.default.min.css" %}
    {% include "assets/css/slick.css" %}
    {% include "assets/css/slick-theme.css" %}
    {% include "assets/css/style.css" %}
    {% include "assets/css/colors/default.css" %}
  {% endset %}
  <style>{{ css | cssmin | safe }}</style>
  {# Uncomment below if you want to include minified inline JavaScript #}
  {# {% set js %}
    {% include "assets/js/inline.js" %}
  {% endset %}
  <script>{{ js | jsmin | safe }}</script> #}
</head>
